<Window x:Class="VMSharp.help"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VMSharp"
        mc:Ignorable="d"
        Title="help" Height="300" Width="400" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen">
    <Grid Background="#FF2D2D2D">
        <TextBlock x:Name="help1" HorizontalAlignment="Center" TextWrapping="Wrap" Text="TextBlock"  FontSize="16"  Foreground="#FF517BFF" Margin="10,10,10,10" FontFamily="Consolas"/>

    </Grid>
</Window>
